{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "globals": {
      "expect": true,
      "test": true,
      "describe": true,
      "afterEach": true,
      "beforeEach": true,
      "it": true,
      "beforeAll": true,
      "afterAll": true,
    },
    "rules": {
      "no-underscore-dangle": off,
      "no-console": off
    }
}
